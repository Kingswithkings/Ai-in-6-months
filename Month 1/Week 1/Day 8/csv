import csv
rows = [
    ['name', 'age', 'score'],
    ['Alice', '23', '88'],
    ['Kings', '34', '95'],
    ['Carol', '27', '79']
    ]
with open('people.csv', 'w', newline='', encoding='utf-8') as f:
    writer = csv.writer(f)
    writer.writerows(rows)
print('Wrote people.csv')

# Read CSV
with open('people.csv', 'r', encoding='utf-8') as f:
    reader = csv.reader(f)
    for r in reader:
        print(r)